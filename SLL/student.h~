#ifndef STUDENT_H
#define STUDENT_H

class student {
 public:
  student(char* newFirstName, char* newLastName, int newID, float newGPA);
  char* getFirstName();
  char* getLastName();
  int getID();
  float getGPA();
  ~student();
 private:
  char* firstName;
  char* lastName;
  int ID;
  float GPA;
};

#endif
